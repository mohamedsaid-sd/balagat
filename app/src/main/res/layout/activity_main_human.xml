<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".main_human">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="UselessParent">

            <LinearLayout
                android:id="@+id/lay_main"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/circle_white"
                android:orientation="vertical"
                android:paddingBottom="20dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:orientation="horizontal"
                    tools:ignore="RtlHardcoded,UseCompoundDrawables">

                    <TextView
                        android:id="@+id/txt_name"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="20dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/jana"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="الموارد البشرية"
                        android:textColor="@color/black"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        app:drawableStartCompat="@drawable/ic_baseline_supervised_user_circle_24"
                        tools:ignore="HardcodedText,UseCompatTextViewDrawableXml" />

                    <ImageView
                        android:id="@+id/imageView2"
                        android:layout_width="wrap_content"
                        android:layout_height="50dp"
                        android:layout_margin="15dp"
                        android:background="@drawable/whiteroundblack"
                        android:contentDescription="@string/Image"
                        android:onClick="fun_back"
                        android:padding="5dp"
                        app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                        tools:ignore="ContentDescription,TouchTargetSizeCheck" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lay_human"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txt_salary"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:onClick="fun_cat_employee"
                            android:padding="5dp"
                            android:text="التوظيف"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:id="@+id/textView16"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:onClick="fun_cat_borrow"
                            android:padding="5dp"
                            android:text="المستحقات والسلفيات"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView190"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:drawableTop="@drawable/ic_baseline_supervised_user_circle_24"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:onClick="fun_cat_prose"
                            android:padding="5dp"
                            android:text="العهد والنثريات"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText,NestedWeights,UseCompatTextViewDrawableXml" />

                        <TextView
                            android:id="@+id/txt_resignation"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:onClick="fun_cat_trainig"
                            android:padding="5dp"
                            android:text="الحضور والاداء"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView1590"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:onClick="fun_cat_development"
                            android:padding="5dp"
                            android:text="التدريب والتطوير"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText,NestedWeights" />

                        <TextView
                            android:id="@+id/txt_complant"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:onClick="fun_complant_request"
                            android:padding="5dp"
                            android:text="تقديم شكوي  "
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            android:visibility="invisible"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lay_opration"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:drawableTop="@drawable/ic_baseline_supervised_user_circle_24"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text="التاجير"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            android:onClick="fun_rental"
                            tools:ignore="HardcodedText,UseCompatTextViewDrawableXml" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text="الإنتاج"
                            android:onClick="fun_production"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text="المقاولة"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            android:onClick="fun_constraction"

                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText,NestedWeights" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text="الحضور والاداء"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            android:visibility="invisible"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lay_fleet"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text="المعدات الثقيلة"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text="الشاحنات والقلابات"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:onClick="fun_maintenance"
                            android:padding="5dp"
                            android:text="الصيانة"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText,NestedWeights" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text="قطع الغيار"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            android:visibility="visible"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lay_supply"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:onClick="fun_purchases"
                            android:padding="5dp"
                            android:text="المشتروات"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text="الترحيل"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:onClick="fun_order_deportation"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:onClick="fiun_stock"
                            android:padding="5dp"
                            android:text="المخزن"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:onClick="fun_out_equipment_contract"
                            android:padding="5dp"
                            android:text="عقودات المعدات الخارجية"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lay_office"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text="مكتب عطبرة"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text="موقع قبقبة"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lay_clients"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text=" التواصل المستمر "
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/whiteroundblack"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:padding="5dp"
                            android:text=" الشكاوى والمقترحات "
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:drawableTopCompat="@drawable/ic_baseline_supervised_user_circle_24"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>

                </LinearLayout>

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/grayround"
                    android:fontFamily="@font/jana"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="مع تحيات فريق الموارد البشرية"
                    android:textColor="@color/lite"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    tools:ignore="HardcodedText,InefficientWeight" />




            </LinearLayout>
        </ScrollView>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
